    <script>
        document.addEventListener('DOMContentLoaded', function () {
            // Tab switching
            const tabButtons = document.querySelectorAll('.tab-button');
            const tabContents = document.querySelectorAll('.tab-content');

            tabButtons.forEach(button => {
                button.addEventListener('click', () => {
                    const targetTab = button.dataset.tab;

                    tabButtons.forEach(btn => {
                        btn.classList.remove('tab-active', 'text-indigo-600', 'border-indigo-500');
                        btn.classList.add('text-gray-500', 'border-transparent', 'hover:text-gray-700', 'hover:border-gray-300');
                    });
                    button.classList.add('tab-active', 'text-indigo-600', 'border-indigo-500');
                    button.classList.remove('text-gray-500', 'border-transparent', 'hover:text-gray-700', 'hover:border-gray-300');
                    
                    tabContents.forEach(content => {
                        if (content.id === `tab-content-${targetTab}`) {
                            content.classList.remove('hidden');
                        } else {
                            content.classList.add('hidden');
                        }
                    });
                });
            });

            // Chart Initializations
            // 1. Keyword Rankings Over Time (ApexCharts - Line)
            if (document.getElementById('keyword-rankings-chart')) {
                var keywordRankingsOptions = {
                    series: [{
                        name: "Avg. Rank",
                        data: [10, 12, 8, 9, 7, 6, 5, 4.5, 4.2, 4]
                    }],
                    chart: {
                        height: 350,
                        type: 'line',
                        zoom: { enabled: false },
                        toolbar: { show: false }
                    },
                    dataLabels: { enabled: false },
                    stroke: { curve: 'smooth', width: 3 },
                    xaxis: {
                        categories: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct'],
                    },
                    yaxis: { reversed: true, min:1, max:20, tickAmount: 5, labels: { formatter: (val) => Math.round(val) } },
                    colors: ['#4f46e5'],
                    tooltip: { y: { formatter: (val) => "Rank: " + val } }
                };
                var keywordRankingsChart = new ApexCharts(document.querySelector("#keyword-rankings-chart"), keywordRankingsOptions);
                keywordRankingsChart.render();
            }

            // 2. LLM Visibility Score (ApexCharts - Radial Bar)
            if (document.getElementById('llm-visibility-chart')) {
                 var llmVisibilityOptions = {
                    series: [78, 65, 52, 45],
                    chart: {
                        height: 350,
                        type: 'radialBar',
                    },
                    plotOptions: {
                        radialBar: {
                            offsetY: 0,
                            startAngle: 0,
                            endAngle: 270,
                            hollow: {
                                margin: 5,
                                size: '30%',
                                background: 'transparent',
                                image: undefined,
                            },
                            dataLabels: {
                                name: { show: false },
                                value: { show: false }
                            }
                        }
                    },
                    colors: ['#10B981', '#3B82F6', '#8B5CF6', '#F59E0B'], // Green, Blue, Purple, Amber
                    labels: ['ChatGPT', 'Gemini', 'Claude', 'Perplexity'],
                    legend: {
                        show: true,
                        floating: true,
                        fontSize: '12px',
                        position: 'left',
                        offsetX: 50,
                        offsetY: 15,
                        labels: { useSeriesColors: true, },
                        markers: { size: 0 },
                        formatter: function(seriesName, opts) {
                            return seriesName + ":  " + opts.w.globals.series[opts.seriesIndex] + '%'
                        },
                        itemMargin: { vertical: 3 }
                    },
                    responsive: [{
                        breakpoint: 480,
                        options: {
                            legend: { show: false }
                        }
                    }]
                };
                var llmVisibilityChart = new ApexCharts(document.querySelector("#llm-visibility-chart"), llmVisibilityOptions);
                llmVisibilityChart.render();
            }

            // 3. Revenue by Channel (Chart.js - Bar)
            if (document.getElementById('revenue-chart')) {
                const revenueCtx = document.getElementById('revenue-chart').getContext('2d');
                new Chart(revenueCtx, {
                    type: 'bar',
                    data: {
                        labels: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun'],
                        datasets: [{
                            label: 'Organic',
                            data: [12000, 19000, 15000, 22000, 18000, 25000],
                            backgroundColor: 'rgba(79, 70, 229, 0.7)', // Indigo
                        }, {
                            label: 'Paid',
                            data: [8000, 12000, 9000, 15000, 10000, 16000],
                            backgroundColor: 'rgba(59, 130, 246, 0.7)', // Blue
                        }, {
                            label: 'Referral',
                            data: [5000, 7000, 6000, 8000, 7500, 9000],
                            backgroundColor: 'rgba(16, 185, 129, 0.7)', // Green
                        }]
                    },
                    options: {
                        responsive: true,
                        maintainAspectRatio: false,
                        scales: {
                            x: { stacked: true },
                            y: { stacked: true, beginAtZero: true, ticks: { callback: value => `$${value/1000}k` } }
                        },
                        plugins: { legend: { position: 'top' } }
                    }
                });
            }

            // 4. Brand Sentiment Analysis (Chart.js - Doughnut)
            if (document.getElementById('brand-sentiment-chart')) {
                const sentimentCtx = document.getElementById('brand-sentiment-chart').getContext('2d');
                new Chart(sentimentCtx, {
                    type: 'doughnut',
                    data: {
                        labels: ['Positive', 'Negative', 'Neutral'],
                        datasets: [{
                            label: 'Brand Sentiment',
                            data: [65, 15, 20], // Example percentages
                            backgroundColor: [
                                'rgba(16, 185, 129, 0.8)', // Green
                                'rgba(239, 68, 68, 0.8)',  // Red
                                'rgba(245, 158, 11, 0.8)' // Amber
                            ],
                            borderColor: [
                                'rgba(16, 185, 129, 1)',
                                'rgba(239, 68, 68, 1)',
                                'rgba(245, 158, 11, 1)'
                            ],
                            borderWidth: 1
                        }]
                    },
                    options: {
                        responsive: true,
                        maintainAspectRatio: false,
                        plugins: { legend: { position: 'bottom' } }
                    }
                });
            }
        });
    </script>
</body>
</html>
